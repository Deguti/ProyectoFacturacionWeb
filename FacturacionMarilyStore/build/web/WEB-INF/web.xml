<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>EliminarClientes</servlet-name>
        <servlet-class>Servlets.EliminarClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearModifClientes</servlet-name>
        <servlet-class>Servlets.CrearModifClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarProducto</servlet-name>
        <servlet-class>Servlets.AgregarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarProducto</servlet-name>
        <servlet-class>Servlets.EliminarProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarDetalle</servlet-name>
        <servlet-class>Servlets.EliminarDetalle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelarFact</servlet-name>
        <servlet-class>Servlets.CancelarFact</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Facturar</servlet-name>
        <servlet-class>Servlets.Facturar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearPDF</servlet-name>
        <servlet-class>Servlets.CrearPDF</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EliminarClientes</servlet-name>
        <url-pattern>/EliminarClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearModifClientes</servlet-name>
        <url-pattern>/CrearModifClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarProducto</servlet-name>
        <url-pattern>/AgregarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarProducto</servlet-name>
        <url-pattern>/EliminarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarDetalle</servlet-name>
        <url-pattern>/EliminarDetalle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelarFact</servlet-name>
        <url-pattern>/CancelarFact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Facturar</servlet-name>
        <url-pattern>/Facturar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearPDF</servlet-name>
        <url-pattern>/CrearPDF</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
